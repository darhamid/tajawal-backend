{
  "name": "tajawal-hotelapp-service",
  "version": "0.0.1",
  "scripts": {
    "prodstart": "export NODE_ENV=production && npm run start",
    "stagingstart": "export NODE_ENV=staging && npm run start",
    "teststart": "export NODE_ENV=test && npm run start",
    "dev": "./node_modules/.bin/nodemon -L -e .ts -w src --exec \"./node_modules/ts-node/dist/bin.js src\"",
    "build": "./node_modules/.bin/tsc --p tsconfig.json",
    "start": "node dist --max-old-space-size=8192 â€“max-new-space-size=2048",
    "prestart": "npm install",
    "test": "./node_modules/.bin/nyc mocha 'test/**/*.spec.ts'",
    "lint": "tslint src/*/*.ts --format codeframe",
    "fix-lint": "tslint src/*/*.ts --fix",
    "esw": "esw src/*/*.ts --format codeframe --watch",
    "type-check": "tsc --noEmit"
  },
  "lint-staged": {
    "*.{ts,js,json,css,md}": [
      "prettier --write",
      "git add"
    ]
  },
