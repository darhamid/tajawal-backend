    "babel-runtime": "^6.26.0",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "mocha": "^7.0.1",
    "nock": "^11.4.0",
    "node-mocks-http": "^1.8.0",
    "nodemon": "^2.0.2",
    "nyc": "^15.0.0",
