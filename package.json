    "@types/jsonwebtoken": "^8.3.5",
    "@types/lodash": "^4.14.144",
    "@types/log4js": "^2.3.5",
    "@types/mocha": "^7.0.1",
    "@types/mongoose": "^5.5.22",
    "@types/node-cron": "^2.0.2",
    "@types/request": "^2.48.3",
    "@types/sinon": "7.5.1",
