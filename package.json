    "lodash": "^4.17.15",
    "log4js": "^6.1.2",
    "moment": "^2.24.0",
    "mongoose": "5.8.11",
    "swagger-ui-express": "^4.1.2",
    "uuid": "^3.3.3"
  },
  "devDependencies": {
    "@types/assert": "^1.4.3",
